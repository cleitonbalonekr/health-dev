{
  "name": "Affluens",
  "description": "Focus on yous tasks",
  "version": "0.0.0.4",
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "src/infra/gateways/chrome-alarm-service.ts",
    "type": "module"
  },
  "icons": {
    "16": "logo48.png",
    "48": "logo48.png",
    "128": "logo512.png"
  },
  "permissions": ["storage", "alarms", "notifications"]
}
