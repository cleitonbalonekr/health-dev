{
  "name": "Helth Concentration",
  "description": "Focus on yous tasks, we'll help you be health",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "src/infra/gateways/chrome-alarm-service.ts"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["storage", "alarms", "notifications"]
}
